<h1>Random Beer</h1>
<div class= "beers-container">
{{#each randomBeer}} 
<div class="beers-detail">
<h3>{{name}} </h3>
<p>{{tagline}} </p>
<img src="{{image_url}} " alt=" {{name}}">

<p>Description: {{description}} </p> 
<ul>
{{#each food_pairing}}
<li>{{this</li>

{{/each}}
</ul>
<p>Tips: {{brewers_tips}}</p>
{{/each}}
</div>


</div>